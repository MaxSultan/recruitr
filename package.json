{
  "name": "recruitr",
  "version": "1.0.0",
  "description": "Track Wrestling data scraper with Express server",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "setup-db": "node scripts/setup-db.js",
    "setup-db:force": "node scripts/setup-db.js --force",
    "setup-db:migrate": "node scripts/setup-db.js --migrate",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "migrate:status": "npx sequelize-cli db:migrate:status",
    "migrate:create": "npx sequelize-cli migration:generate --name",
    "scrape:utah": "node scripts/scrape-utah-tournaments.js",
    "scrape:idaho": "node scripts/scrape-idaho-tournaments.js",
    "scrape:nevada": "node scripts/scrape-nevada-tournaments.js",
    "scrape:arizona": "node scripts/scrape-arizona-tournaments.js",
    "scrape:colorado": "node scripts/scrape-colorado-tournaments.js",
    "scrape:all": "npm run scrape:utah && npm run scrape:idaho && npm run scrape:nevada && npm run scrape:arizona && npm run scrape:colorado"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "sequelize": "^6.35.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.3"
  },
  "keywords": [
    "wrestling",
    "scraping",
    "express",
    "tournament"
  ],
  "author": "",
  "license": "MIT"
}
