{
  "name": "recruitr",
  "version": "1.0.0",
  "description": "Track Wrestling data scraper with Express server",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "setup-db": "node scripts/setup-db.js",
    "setup-db:force": "node scripts/setup-db.js --force",
    "setup-db:migrate": "node scripts/setup-db.js --migrate",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "migrate:status": "npx sequelize-cli db:migrate:status",
    "migrate:create": "npx sequelize-cli migration:generate --name"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "puppeteer": "^21.6.1",
    "sequelize": "^6.35.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.3"
  },
  "keywords": [
    "wrestling",
    "scraping",
    "express",
    "tournament"
  ],
  "author": "",
  "license": "MIT"
}
